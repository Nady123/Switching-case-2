#include<stdio.h>
#include<conio.h>
main()
{
	printf("Enter you grade is :");
	char grade ='B';
	scanf("%c", &grade);
	
	switch(grade)
	{
		case'A':
			printf("Excellent!\n");
			break;
		case'B':
			printf("Well done\n");
			break;
		case'C':
			printf("Good\n");
			break;
		case'D':
			printf("Better\n");
			break;
		case'F':
			printf("Better luck next time\n");
			break;
		default:
			printf("Grade is invalid\n");
			
	}
}
